{

  "parser": "babel-eslint",

  "extends": "eslint:recommended",

  "rules": {
    "no-unused-expressions": 1,
    "no-unused-vars": 1,
    "strict": 0,
    "new-cap": 0,
    "camelcase": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 2
  },

  "ecmaFeatures": {
    "jsx": true
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },

  "globals": {
    "BROWSER": true
  },

  "plugins": ["react"]

}
